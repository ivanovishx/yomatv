<volist name="adminmenus" id="vo">
<h3 class="f14"><span class="switchs cu on" title="展开与收缩"></span>{$vo['menuname']}</h3>
<ul>
	<volist name="vo['voo']" id="sub">
	<li id="_MP{$sub['id']}" class="sub_menu"><a href="javascript:_MP({$sub['id']},'{$sub['url']}');" hidefocus="true" style="outline:none;">{$sub['menuname']}</a></li>
	</volist>
</ul>
</volist>
<if condition = "$_GET['menuid'] eq 1">
<h3 class="f14"><span class="switchs cu on" title="展开与收缩"></span>快捷菜单</h3>
<ul>
	<volist name="adminqmenus" id="vo">
	<li id="_MP{$vo['menuid']}" class="sub_menu"><a href="javascript:_MP({$vo['menuid']},'{$vo['url']}');" hidefocus="true" style="outline:none;">{$vo['menuname']}</a></li>
	</volist>
</ul>
</if>
<script type="text/javascript">
$(".switchs").each(function(i){
	var ul = $(this).parent().next();
	$(this).click(
	function(){
		if(ul.is(':visible')){
			ul.hide();
			$(this).removeClass('on');
				}else{
			ul.show();
			$(this).addClass('on');
		}
	})
});
</script>